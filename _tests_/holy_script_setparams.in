#! @builddir@/holy-shell-tester


if test x$holyshell = xyes; then cmd=setparams; else cmd=set; fi

function f1 {
    echo $#
    echo "$#"

    echo $@
    echo "$@"

    echo $*
    echo "$*"

    echo $1 $2
    for v in "$@"; do echo $v; done
    shift
    echo $1 $2
    for v in "$@"; do echo $v; done

    $cmd 1 2 3 4

    echo $#
    echo "$#"

    echo $@
    echo "$@"

    echo $*
    echo "$*"

    echo $1 $2
    for v in "$@"; do echo $v; done
    shift
    echo $1 $2
    for v in "$@"; do echo $v; done
}
